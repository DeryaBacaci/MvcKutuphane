@using MvcKutuphane.Models.Entity
@model List<TBLMESAJLAR>
@{
    ViewBag.Title = "Giden";
    Layout = "~/Views/Shared/_LayoutPanelim.cshtml";
}

<h2>Giden Mesajlar Sayfası</h2>
<br />
<table class="table table-bordered">
    <tr>
        <th>Alıcı</th>
        <th>Konu</th>
        <th>Mesaj</th>
        <th>İşlem</th>
    </tr>

    @foreach (var m in Model)
    {
        <tr>
            <td>@m.ALICI</td>
            <td>@m.KONU</td>
            <td>@m.ICERIK</td>
            <td>@m.TARIH</td>
        </tr>

    }

</table>

